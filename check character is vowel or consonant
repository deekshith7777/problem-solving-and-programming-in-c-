#include <stdio.h>
#include<conio.h>
int main()
{
char ch;    
    printf("enter character \n");
    scanf("%c",&ch);
    if(ch=='a' || ch=='e' || ch=='i' || ch=='o' || ch=='u' || ch=='A' || ch=='E' || ch=='I' || ch=='O' || ch=='U')
{
    printf("\n character is vowel");
}
else
{
printf("\n character is consonant");
 }  
 return 0;
}
