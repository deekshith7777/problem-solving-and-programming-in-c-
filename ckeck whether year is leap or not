#include <stdio.h>

int main()
{
    int a;
    printf("enter year");
    scanf("%d",&a);
    if(a%400==0)
    {
        printf("the year is leap year");
}
else if(a%100==0)
{
    printf("the year is  not leap year");
}
else if(a%4==0)
{
    printf(" the year is leap year");
}
else
{
    printf("not a leap");
}
    return 0;
}
